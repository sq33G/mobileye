    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug Tests (jobs)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pipe_dream/manage.py",
            "args": [
                "test",
                "jobs"
            ],
            "django": true
        },
        {
            "name": "Python: Debug Task Runner",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pipe_dream/manage.py",
            "args": [
                "process_tasks",
                "--duration",
                "10"
            ],
            "django": true
        }
    ]
}